                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Membros.Nome, Membros.DataNascimento, Membros.GrupoDia, Membros.GrupoHorario, Membros.Email, Membros.Telefone1, Membros.Telefone2, Membros.Telefone3, Membros.Telefone4, 
